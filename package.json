{
  "name": "yuvjs",
  "version": "1.0.1",
  "description": "a library for reading, writing and converting YUV frames",
  "main": "./src/index.ts",
  "exports": {
    ".": "./dist/index.js",
    "./web": "./dist/web.js"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "web": [
        "./dist/web.d.ts"
      ]
    }
  },
  "files": [
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/parence/yuvjs.git"
  },
  "keywords": [
    "yuv",
    "yuv420",
    "yuv444",
    "yuv400",
    "ycbcr",
    "rgb",
    "color conversion",
    "frame"
  ],
  "author": "Nicola Giuliani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parence/yuvjs/issues"
  },
  "homepage": "https://github.com/parence/yuvjs#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.7.0",
    "jest": "^27.4.5",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
